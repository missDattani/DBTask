@model SchoolManagement_SIT326.Models.Models.CityModel
@{
    ViewBag.Title = "GetCityById";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>GetCityById</h2>
@if(TempData["City"] != null)
            {
                <div class="alert alert-danger">@TempData["City"]</div>
            }
<table class="table">
    <tr>
        <th>CityName</th>
        <th>StateName</th>
        <th>CountryName</th>
    </tr>
    <tr>
        <td>@Model.CityName</td>
        <td>@Model.StateName</td>
        <td>@Model.CountryName</td>
    </tr>
</table>

<div>
    @Html.ActionLink("Back To List","GetCities")
</div>