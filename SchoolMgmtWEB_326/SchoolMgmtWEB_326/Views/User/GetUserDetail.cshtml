@model SchoolMgmtWEB_326.Models.Models.UserModel 
@{
    ViewBag.Title = "GetUserDetail";
    Layout = "~/Views/Shared/LayoutMain.cshtml";
}
<style>
    th,td{
        color:white;
    }
</style>
<h2>GetUserById</h2>
<table class="table">
    <tr>
        <th>FirstName</th>
        <th>LastName</th>
        <th>Email</th>
        <th>Role</th>
