    <tr>
        <td>@Model.FirstName</td>
        <td>@Model.LastName</td>
        <td>@Model.Email</td>
        <td>@Model.Role</td>
    </tr>
</table>
<br />
<div>
    @Html.ActionLink("Back To List","GetUsers")
</div>

