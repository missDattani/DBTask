<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Title Page-->
    <title>Dashboard</title>
  
    <!-- Bootstrap CSS-->
    <link href="~/Content/Theme/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="~/Content/Theme/css/theme.css" rel="stylesheet" media="all">

</head>

<body class="animsition">
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        <header class="header-mobile d-block d-lg-none">
            <div class="header-mobile__bar">
                <div class="container-fluid">
                    <div class="header-mobile-inner">
                        <h3>CRUD</h3>
                        <button class="hamburger hamburger--slider" type="button">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <nav class="navbar-mobile">
                <div class="container-fluid">
                    <ul class="navbar-mobile__list list-unstyled">
                        <li>
                            <a href="/Student/Index">Students</a>
                        </li>
                        <li>
                            <a href="/Country/CountryIndex">Countries</a>
                        </li>
                        <li>
                            <a href="/State/StateIndex">States</a>
                        </li>
                        <li>
                            <a href="/City/CityIndex">Cities</a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- END HEADER MOBILE-->
        <!-- MENU SIDEBAR-->
        <div class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <h3>CRUD</h3>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list">
                        <li>
                            <a href="/Student/Index">Students</a>
                        </li>
                        <li>
                            <a href="/Country/CountryIndex">Countries</a>
                        </li>
                        <li>
                            <a href="/State/StateIndex">States</a>
                        </li>
                        <li>
                            <a href="/City/CityIndex">Cities</a>
                        </li>
                    </ul>
                </nav>
            </div>
        </div>
        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <div class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap" style="justify-content:end;">
                            <div class="header-button">
                                <div class="noti-wrap">
                                    <a href="/student/Logout" class="btn btn-outline-dark">Logout</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
            <div class="main-content">
                <div class="container-fluid">
                    <div class="row">
                        @RenderBody()
                    </div>
                </div>
            </div>
            <!-- END MAIN CONTENT-->
            <!-- END PAGE CONTAINER-->
        </div>

    </div>

    <!-- Jquery JS-->
    <script src="~/Content/Theme/vendor/jquery-3.2.1.min.js"></script>
    <script src="~/Content/Theme/vendor/animsition/animsition.min.js"></script>
    <!-- Main JS-->
    <script src="~/Content/Theme/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/Content/Theme/js/main.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
</body>

</html>
<!-- end document-->
